#. (itstool) path: plugin/_short
msgid "Output Switcher"
msgstr "Schimbător de ecrane"

#. (itstool) path: plugin/_long
msgid "A plugin to change the focused output."
msgstr "Un modul pentru a schimba ecranul focalizat."

#. (itstool) path: option/_short
msgid "Next output"
msgstr "Ecranul următor"

#. (itstool) path: option/_long
msgid "Moves focus to the next output with the specified activator."
msgstr "Mută focalizarea la ecranul următor cu activatorul specificat."

#. (itstool) path: option/_short
msgid "Next output with window"
msgstr "Ecranul următor cu fereastră"

#. (itstool) path: option/_long
msgid ""
"Moves focus to the next output with the focused window with the specified "
"activator."
msgstr ""
"Mută focalizarea la ecranul următor împreună cu fereastra focalizată, cu "
"activatorul specificat."

#. (itstool) path: option/_short
msgid "Previous output"
msgstr "Ecranul anterior"

#. (itstool) path: option/_long
msgid "Moves focus to the previous output with the specified activator."
msgstr "Mută focalizarea la ecranul anterior cu activatorul specificat."

#. (itstool) path: option/_short
msgid "Previous output with window"
msgstr "Ecranul anterior cu fereastră"

#. (itstool) path: option/_long
msgid ""
"Moves focus to the previous output with the focused window with the "
"specified activator."
msgstr ""
"Mută focalizarea la ecranul anterior împreună cu fereastra focalizată, cu "
"activatorul specificat."

#. (itstool) path: option/_short
msgid "Left output"
msgstr "Ecranul din stânga"

#. (itstool) path: option/_long
msgid "Moves focus to the output on the left."
msgstr "Mută focalizarea la ecranul din stânga."

#. (itstool) path: option/_short
msgid "Right output"
msgstr "Ecranul din dreapta"

#. (itstool) path: option/_long
msgid "Moves focus to the output on the right."
msgstr "Mută focalizarea la ecranul din dreapta."

#. (itstool) path: option/_short
msgid "Above output"
msgstr "Ecranul de deasupra"

#. (itstool) path: option/_long
msgid "Moves focus to the output above."
msgstr "Mută focalizarea la ecranul de deasupra."

#. (itstool) path: option/_short
msgid "Below output"
msgstr "Ecranul de dedesubt"

#. (itstool) path: option/_long
msgid "Moves focus to the output below."
msgstr "Mută focalizarea la ecranul de dedesubt."
